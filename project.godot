; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="SpaceBash"
run/main_scene="res://scenes/start_menu.tscn"
config/features=PackedStringArray("4.0", "GL Compatibility")
boot_splash/bg_color=Color(1, 1, 1, 1)
boot_splash/image="res://assets/intropg.png"
config/icon="res://icon.svg"
config/windows_native_icon="res://assets/ui/icon.ico"
boot_splash/minimum_display_time=3000

[autoload]

GameManager="*res://scenes/game_manager.tscn"
SaveLoad="*res://scripts/save_load.gd"
Music="*res://scenes/music.tscn"

[display]

window/size/viewport_width=480
window/size/viewport_height=854
window/handheld/orientation=1
window/stretch/mode="viewport"

[input]

serve={
"deadzone": 0.5,
"events": [Object(InputEventMouseButton,"resource_local_to_scene":false,"resource_name":"","device":-1,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":false,"meta_pressed":false,"button_mask":0,"position":Vector2(0, 0),"global_position":Vector2(0, 0),"factor":1.0,"button_index":1,"pressed":false,"double_click":false,"script":null)
]
}

[input_devices]

pointing/emulate_touch_from_mouse=true

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/vram_compression/import_s3tc_bptc=false
textures/vram_compression/import_etc2_astc=true
environment/defaults/default_clear_color=Color(0.0784314, 0.0705882, 0.137255, 1)
